<Window x:Class="Semenova_Language.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Semenova_Language"
        mc:Ignorable="d"
        Title="Semenova_Language" MinHeight="450" MinWidth="800" Icon="Resources\school_logo.ico">
    <Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="75"></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
            </Grid.RowDefinitions>
            <TextBlock Text="Список клиентов" FontSize="40" HorizontalAlignment="Center" ></TextBlock>
            <Image Source="Resources\school_logo.png" HorizontalAlignment="Left"></Image>
            <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered"></Frame>
            <Button Content="Назад" Name="BtnBack" Click="BtnBack_Click"  HorizontalAlignment="Right"></Button>
        </Grid>
    </Grid>
</Window>
